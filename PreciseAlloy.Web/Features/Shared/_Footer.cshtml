@model PreciseAlloy.Web.Features.ViewComponents.Footer.FooterViewModel

<footer class="zzz-o-footer section-margin-top-xl">
    <div class="zzz-container">
        <div class="zzz-o-footer__inner">
            <ul class="zzz-m-link-list-with-icon">
                <li>
                    <a class="zzz-a-link-with-icon"
                       href="https://www.facebook.com/"
                       aria-label="click to facebook">
                        <svg class="zzz-a-icon zzz-a-icon--md"
                             aria-hidden="true"
                             viewBox="0 0 10 19"
                             width="10"
                             height="19">
                            <use xlink:href="/assets/images/icons.svg#zzz-facebook"></use>
                        </svg>
                    </a>
                </li>
                <li>
                    <a class="zzz-a-link-with-icon"
                       href="https://www.instagram.com/"
                       aria-label="click to instagram">
                        <svg class="zzz-a-icon zzz-a-icon--md"
                             aria-hidden="true"
                             viewBox="0 0 19 19"
                             width="19"
                             height="19">
                            <use xlink:href="/assets/images/icons.svg#zzz-instagram"></use>
                        </svg>
                    </a>
                </li>
                <li>
                    <a class="zzz-a-link-with-icon"
                       href="https://www.twitter.com/"
                       aria-label="click to twitter">
                        <svg class="zzz-a-icon zzz-a-icon--md"
                             aria-hidden="true"
                             viewBox="0 0 19 19"
                             width="19"
                             height="19">
                            <use xlink:href="/assets/images/icons.svg#zzz-twitter"></use>
                        </svg>
                    </a>
                </li>
                <li>
                    <a class="zzz-a-link-with-icon"
                       href="https://www.pinterest.com/"
                       aria-label="click to pinterest">
                        <svg class="zzz-a-icon zzz-a-icon--md"
                             aria-hidden="true"
                             viewBox="0 0 19 19"
                             width="19"
                             height="19">
                            <use xlink:href="/assets/images/icons.svg#zzz-pinterest"></use>
                        </svg>
                    </a>
                </li>
                <li>
                    <a class="zzz-a-link-with-icon"
                       href="https://www.tiktok.com/"
                       aria-label="click to tiktok">
                        <svg class="zzz-a-icon zzz-a-icon--md"
                             aria-hidden="true"
                             viewBox="0 0 19 19"
                             width="19"
                             height="19">
                            <use xlink:href="/assets/images/icons.svg#zzz-tiktok"></use>
                        </svg>
                    </a>
                </li>
                <li>
                    <a class="zzz-a-link-with-icon"
                       href="https://www.whatsapp.com/"
                       aria-label="click to whatsapp">
                        <svg class="zzz-a-icon zzz-a-icon--md"
                             aria-hidden="true"
                             viewBox="0 0 19 19"
                             width="19"
                             height="19">
                            <use xlink:href="/assets/images/icons.svg#zzz-whatsapp"></use>
                        </svg>
                    </a>
                </li>
                <li>
                    <a class="zzz-a-link-with-icon"
                       href="https://www.youtube.com/"
                       aria-label="click to youtube">
                        <svg class="zzz-a-icon zzz-a-icon--md"
                             aria-hidden="true"
                             viewBox="0 0 19 19"
                             width="19"
                             height="19">
                            <use xlink:href="/assets/images/icons.svg#zzz-youtube"></use>
                        </svg>
                    </a>
                </li>
            </ul>
            <div class="zzz-o-footer__copyright">
                <p>© Start, 2022. All rights reserved.</p>
            </div>
        </div>
    </div>
</footer>

@{
    Html.RequireStyle("/assets/css/b-footer.css").AtHeader();
}