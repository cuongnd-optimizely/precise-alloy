{
  "name": "alloy-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "npm run dev",
    "dev": "concurrently --kill-others \"ts-node --esm styles.ts --watch\" \"ts-node --esm states.ts --watch\" \"cross-env scriptOnly=true vite build --mode development --watch\" \"ts-node --esm server.ts --mode development\"",
    "serve": "ts-node --esm server.ts",
    "watch": "ts-node --esm server.ts",
    "build": "tsc && npm run build:static && npm run build:server",
    "build:eshn": "tsc && npm run build:static:eshn && npm run build:server:eshn",
    "preview": "vite preview",
    "build:server": "vite build --ssr src/entry-server.tsx --outDir dist/server",
    "build:server:eshn": "vite build --ssr src/entry-server.tsx --outDir dist/server --mode eshn",
    "build:static": "vite build --outDir dist/static",
    "build:static:eshn": "vite build --outDir dist/static --mode eshn",
    "generate": "ts-node --esm states.ts && npm run styles && npm run build && ts-node --esm prerender.ts --add-hash",
    "eshn": "ts-node --esm states.ts && npm run styles && npm run build:eshn && ts-node --esm prerender.ts --add-hash --mode eshn",
    "inte": "npm run styles && npm run build && ts-node --esm prerender.ts && ts-node --esm integration.ts",
    "styles": "ts-node --esm styles.ts",
    "format": "prettier --write .",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix"
  },
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=16.0.0"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.18.0"
  },
  "devDependencies": {
    "@types/css": "^0.0.36",
    "@types/debounce": "^1.2.4",
    "@types/express": "^4.17.21",
    "@types/glob": "^8.1.0",
    "@types/js-beautify": "^1.14.3",
    "@types/lodash": "^4.14.201",
    "@types/node": "^18.18.9",
    "@types/prettier": "^3.0.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.1.1",
    "autoprefixer": "^10.4.16",
    "chalk": "^5.3.0",
    "cheerio": "^1.0.0-rc.12",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "css": "^3.0.0",
    "cssnano": "^6.0.1",
    "debounce": "^1.2.1",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "express": "^4.18.2",
    "glob": "^10.3.10",
    "js-beautify": "^1.14.11",
    "lodash": "^4.17.21",
    "msw": "^2.0.4",
    "postcss": "^8.4.31",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.0.3",
    "rollup": "^3.29.4",
    "sass": "^1.69.5",
    "slash": "^5.1.0",
    "svgo": "^3.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "vite": "^4.5.0"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
